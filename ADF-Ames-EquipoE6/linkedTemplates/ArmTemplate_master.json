{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Nombre de instancia de Data Factory",
			"defaultValue": "ADF-Ames-EquipoE6"
		},
		"LS_BlobStorage_Output_connectionString": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"connectionString\"de \"LS_BlobStorage_Output\""
		},
		"MongoConn_connectionString": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"connectionString\"de \"MongoConn\""
		},
		"Mongo_BlobStorage_Output_connectionString": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"connectionString\"de \"Mongo_BlobStorage_Output\""
		},
		"NeonConn_password": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"password\"de \"NeonConn\""
		},
		"MongoConn_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "test"
		},
		"NeonConn_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "ep-lingering-river-a85szbey-pooler.eastus2.azure.neon.tech"
		},
		"NeonConn_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "amesDB"
		},
		"NeonConn_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "infrati"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI de la cuenta de almacenamiento que contiene las plantillas de ARM vinculadas."
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Token de SAS para la cuenta de almacenamiento que contiene las plantillas de ARM vinculadas"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF-Ames-EquipoE6_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_BlobStorage_Output_connectionString": {
						"value": "[parameters('LS_BlobStorage_Output_connectionString')]"
					},
					"MongoConn_connectionString": {
						"value": "[parameters('MongoConn_connectionString')]"
					},
					"Mongo_BlobStorage_Output_connectionString": {
						"value": "[parameters('Mongo_BlobStorage_Output_connectionString')]"
					},
					"NeonConn_password": {
						"value": "[parameters('NeonConn_password')]"
					},
					"MongoConn_properties_typeProperties_database": {
						"value": "[parameters('MongoConn_properties_typeProperties_database')]"
					},
					"NeonConn_properties_typeProperties_server": {
						"value": "[parameters('NeonConn_properties_typeProperties_server')]"
					},
					"NeonConn_properties_typeProperties_database": {
						"value": "[parameters('NeonConn_properties_typeProperties_database')]"
					},
					"NeonConn_properties_typeProperties_username": {
						"value": "[parameters('NeonConn_properties_typeProperties_username')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ADF-Ames-EquipoE6_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ADF-Ames-EquipoE6_ArmTemplate_0')]"
			]
		}
	]
}